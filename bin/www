#!/usr/bin/env node

import http from "http";
import app from "../App.js"; 

const port = process.env.PORT || 3000;
app.set("port", port);

// Crear el servidor HTTP
const server = http.createServer(app);


server.listen(port, () => {
  console.log(`Servidor ejecut√°ndose en el puerto ${port}`);
});
